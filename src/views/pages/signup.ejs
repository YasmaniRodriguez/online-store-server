<%- include("../partials/header")-%>
    <section class="page gateway ">
        <div class="wrapper">
            <div class="container signup">
                <span class="material-icons" style="font-size:84px;color:#408282">
                    account_circle
                </span>
                <h3>Sign Up</h3>
                <form action="">
                    <input id="name" name="" type="text" placeholder="Name">
                    <input id="lastname" name="" type="text" placeholder="Lastname">
                    <input id="birthday" name="" type="date" placeholder="Birthday">
                    <input id="avatar" name="" type="file" placeholder="Profile Picture">
                    <input id="phone" name="" type="tel" placeholder="Phone">
                    <input id="email" name="" type="email" placeholder="Email">
                    <input id="address" name="" type="text" placeholder="Address">
                    <input id="password" name="" type="password" placeholder="Password">
                    <input id="confirm" name="" type="password" placeholder="Confirm Password">
                    <button type="submit" id="login-button">Create account</button>
                    <p>By clicking Sign up, you agree to our <a style="text-decoration: none; font-weight: bold;" href="#">Terms and Conditions</a> and <a style="text-decoration: none;font-weight: bold;" href="#">Privacy Statement</a></p>
                </form>
                <div class="links">
                    <a href="/signin">Already have an account?</a>
                </div>
            </div>
        </div>
    </section>
<%- include("../partials/footer")-%>