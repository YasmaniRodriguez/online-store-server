<div id="messenger-container">
    <div id="messenger-header">
        <h2>welcome to public chat for customer support</h2>
    </div>
    <div id="messenger-box">
        <ul id="messages"></ul>
    </div>
    <div id="messenger-form">
        <form id="messenger">
            <input id="message" type="text" name="message" placeholder="Enter text here">
            <button type="submit" id="messenger-submit-button">
                <span class="material-icons-outlined">
                    arrow_upward
                </span>
            </button>
        </form>
    </div>
</div>